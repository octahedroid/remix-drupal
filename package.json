{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "deploy": "wrangler publish",
    "deploy:staging": "wrangler publish --env staging",
    "deploy:production": "wrangler publish --env production",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --env .env.development",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "sync:genql": "npx ts-node scripts/sync-genql.ts"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.7.2",
    "@remix-run/cloudflare-workers": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@tailwindcss/typography": "^0.5.7",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "drupal-auth-client": "^0.2.0",
    "react": "^17.0.2",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@genql/cli": "^3.0.3",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "eslint": "^8.23.1",
    "miniflare": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.22"
  },
  "engines": {
    "node": ">=16.13"
  }
}
